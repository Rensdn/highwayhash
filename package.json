{
  "name": "highwayhash",
  "version": "2.5.0",
  "author": "Lovell Fuller <npm@lovell.info>",
  "description": "Node.js implementation of HighwayHash, Google's fast and strong hash function",
  "homepage": "https://github.com/lovell/highwayhash",
  "scripts": {
    "install": "prebuild-install --runtime=napi || node-gyp rebuild",
    "test": "semistandard && cpplint && node test/unit && prebuild-ci"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/lovell/highwayhash"
  },
  "keywords": [
    "highwayhash",
    "siphash",
    "hash",
    "crypto",
    "random"
  ],
  "dependencies": {
    "node-addon-api": "^3.0.0",
    "prebuild-install": "^5.3.5"
  },
  "devDependencies": {
    "cc": "^2.0.1",
    "long": "^4.0.0",
    "prebuild": "^10.0.0",
    "prebuild-ci": "^3.1.0",
    "semistandard": "^14.2.2"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  },
  "cc": {
    "ignore": [
      "src/highwayhash/**"
    ]
  }
}
